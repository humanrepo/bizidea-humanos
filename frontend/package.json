{
  "name": "bizidea-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend pour la plateforme SaaS Bizidea par HumanOS",
  "homepage": "https://bizidea.com",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
