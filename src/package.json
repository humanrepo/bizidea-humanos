{
  "name": "bizidea-backend",
  "version": "1.0.0",
  "description": "Backend pour la plateforme SaaS Bizidea par HumanOS",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node server.js",
    "test": "echo \"Erreur: aucun test spécifié\" && exit 1",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force node_modules, package-lock.json -ErrorAction SilentlyContinue; npm install\"",
    "clean:bash": "rm -rf node_modules package-lock.json && npm install",
    "dev:windows": "nodemon src/server.js",
    "ports:check": "netstat -an | findstr :5000"
  },
  "keywords": [
    "bizidea",
    "saas",
    "express",
    "postgresql",
    "mongodb",
    "api",
    "backend",
    "francais",
    "français",
    "powershell",
    "windows"
  ],
  "author": "HumanOS",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@prisma/client": "^6.15.0",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "axios": "^1.11.0",
    "eslint": "^8.54.0",
    "jest": "^30.1.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "validator": "^13.15.15"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HumanOS/bizidea-backend.git"
  },
  "bugs": {
    "url": "https://github.com/HumanOS/bizidea-backend/issues"
  },
  "homepage": "https://bizidea.com"
